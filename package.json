{
  "name": "user-dashboard",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build-angular": "ng build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start-angular": "ng serve",
    "start-angular:prod": "ng serve --configuration production",
    "start-nest": "nest start",
    "start-nest:dev": "nest start --watch",
    "start-nest:debug": "nest start --debug --watch",
    "start-nest:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./config/typeorm.config.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./config/typeorm.config.ts migration:generate ./migrations/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./config/typeorm.config.ts migration:revert"
  },
  "dependencies": {
    "@abacritt/angularx-social-login": "^1.2.5",
